{
    "version": 2,
    "builds": [
      {
        "src": "/server/server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/server.js",
        "methods": ["GET", "POST","OPTIONS"],
        "headers":{
          "Access-Control-Allow-Origin":"https://hacky-news-sigma.vercel.app",
          "Access-Control-Allow-Credentials":"true"
        }
      }
    ]
  }